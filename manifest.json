{
	"name": "renatoargh",
	"description": "renatoargh chrome dev tools",
	"version": "1",
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content_script.js"]
    }],
	"icons": {
		"16": "icon.ico",
		"24": "icon.ico",
		"48": "icon.ico"
	},
	"permissions": [
    	"http://*/*",
    	"https://*/*",
    	"contextMenus",
    	"tabs"
	],
	"background": {
    	"scripts": ["background.js"]
	},
	"manifest_version": 2
}
