{
	"name": "Brasil Utils",
	"description": "Ferramentas para desenvolvedores brasileiros",
	"version": "1.0.0",
	"content_scripts": [{
		"matches": ["<all_urls>"],
		"js": ["content_script.js"]
    }],
	"icons": {
		"16": "icon.ico",
		"24": "icon.ico",
		"48": "icon.ico"
	},
	"permissions": [
    	"http://*/*",
    	"https://*/*",
    	"contextMenus",
    	"tabs"
	],
	"background": {
    	"scripts": ["background.js"]
	},
	"manifest_version": 2
}
